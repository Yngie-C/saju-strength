# Supabase
NEXT_PUBLIC_SUPABASE_URL=
NEXT_SUPABASE_PUBLISHABLE_KEY=
NEXT_SUPABASE_SECRET_KEY=

# Build target: 'web' (Vercel SSR) or 'toss' (Static Export)
NEXT_PUBLIC_BUILD_TARGET=web

# API base URL (empty for web/relative, absolute URL for toss static export)
# Web: '' (empty)
# Toss: 'https://saju-strength.vercel.app'
NEXT_PUBLIC_API_BASE_URL=

# Production URL (for toss static export build)
# NEXT_PUBLIC_API_BASE_URL=https://saju-strength.vercel.app

# CORS: Toss WebView origin
NEXT_PUBLIC_TOSS_ORIGIN=https://saju-strength.apps.tossmini.com

# mTLS 인증서 (Base64 인코딩)
TOSS_MTLS_CERT=
TOSS_MTLS_KEY=

# 토스 사용자 데이터 AES-256-GCM 복호화 키 (Base64)
TOSS_DECRYPT_KEY=

# 토스 연결 해제 콜백 Basic Auth
TOSS_DISCONNECT_USERNAME=
TOSS_DISCONNECT_PASSWORD=

# Supabase Service Role Key (서버 사이드 데이터 삭제용)
SUPABASE_SERVICE_ROLE_KEY=

# Sentry DSN (모니터링)
NEXT_PUBLIC_SENTRY_DSN=
